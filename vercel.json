{
    
    "builds": [{
        "src": "otp/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
        "src":"*.env",
        "user": "@vercel/python"
    },
    {
        "src":"build_files.sh",
        "use":"@vercel/python",
        "config":{
            "distDir":"staticfiles_build"
        }
    }
],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "otp/wsgi.py"
        }
    ]
}